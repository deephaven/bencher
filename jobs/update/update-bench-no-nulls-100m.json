              {
                  "title" : "setup metrics",
                  "file" : "before.py",
                  "timed" : 0
              },

              {
                  "title" : "perform update",
                  "text" : [
                      "time_start_ns = time.perf_counter_ns()",
                      "result = relation.update('composite_id = 643*adjective_id + animal_id')",
                      "time_end_ns = time.perf_counter_ns()"
                  ],
                  "timed" : 1
              },

              {
                  "title" : "post processing",
                  "file" : "after.py",
                  "timed" : 0
              }
          ]
      }
